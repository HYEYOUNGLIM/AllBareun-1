<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.allbareun.web.dao.GoalCategoryDao">
	<insert id="insert">
		INSERT INTO GoalCategory
		(
			`goalId`,
			`goalCategoryTypeId`,
			`priority`
		)
		VALUES
		(
			#{goalId},
			#{goalCategoryTypeId},
			#{priority}
		);
	</insert>
	
	<delete id="delete">
		DELETE FROM GoalCategory
		WHERE goalId = ${goalId};
	</delete>
</mapper>